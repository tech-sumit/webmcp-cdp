{
  "name": "@tech-sumit/webmcp-cdp",
  "version": "0.1.0",
  "description": "Node.js library for WebMCP tool discovery and execution via Chrome DevTools Protocol",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "format": "prettier --write 'src/**/*.ts'",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "chrome-remote-interface": "^0.34.0",
    "@tech-sumit/ai-inspector-types": "file:../ai-inspector-types"
  },
  "devDependencies": {
    "@types/chrome-remote-interface": "^0.31.14",
    "devtools-protocol": "^0.0.1502260",
    "tsup": "^8.0.0",
    "typescript": "^5.6.0",
    "vitest": "^3.0.0",
    "eslint": "^9.0.0",
    "prettier": "^3.0.0"
  },
  "keywords": ["webmcp", "cdp", "chrome-devtools-protocol", "tool-discovery"],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tech-sumit/webmcp-cdp"
  }
}
